modules = ["python-3.11", "postgresql-16"]

[nix]
channel = "stable-24_05"
packages = ["ffmpeg-full", "libxcrypt", "openssl", "postgresql"]

[deployment]
deploymentTarget = "autoscale"
run = ["gunicorn", "--bind", "0.0.0.0:5000", "main:app"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

